@page
@model PrivacyModel
@{
    ViewData["Title"] = "Add Record";
}
<h1>@ViewData["Title"]</h1>

<div class="text-center">


    <!-- Floating Labels Form -->
    <form class="row g-3" Model="Input" OnValidSubmit="Submit" method="post">
        <div class="col-md-6">
            <div class="form-floating">
                <input asp-for="Customer.PlotNo" class="form-control" aria-required="true" />
                <label for="floatingEmail">Plot No</label>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-floating">
                <input asp-for="Customer.Zone" class="form-control" aria-required="true" />
                <label for="floatingEmail">Cadastral Zone</label>
            </div>
        </div>

        <div class="col-md-6">
            <div class="form-floating">
                <input asp-for="Customer.File_No" class="form-control" aria-required="true" />
                <label for="floatingEmail">File No</label>
            </div>
        </div>

        <div class="col-md-6">
            <div class="form-floating">
                <input asp-for="Customer.DeveloperName" class="form-control" aria-required="true" />
                <label for="floatingEmail">Developer Name</label>
            </div>
        </div>

        <div class="col-md-12">
            <button type="submit" class="btn btn-primary ">Save</button>
        </div>
    </form>

  

</div>


